<div class="card mb-4">
    {{#with navigation}}
    <div class="nav">
        {{#each links}}
        <div class="nav-item">
            <a href={{#if href}} {{href}} {{else}} "#" {{/if}}
                class="nav-link {{#if @last}} disabled {{/if}}">{{text}}</a>
        </div>
        {{/each}}
    </div>
    {{/with}}
</div>