<main>
  <div class='card'>
    <div class='card-body'>
      {{#with heading}}
      <div class='card'>
        <div class='card-body'>
          {{#with main}}
          <h1>{{title}}</h1>
          <p>{{text}}</p>
          {{#with button_text}}
          <button class='btn btn-primary'>{{solid}}</button>
          <br />
          <br />
          <button class='btn btn-outline-primary'>{{outline}}</button>
          {{/with}}
          {{/with}}
        </div>
      </div>
      <br />
      <div class='card'>
        <div class='card-body'>
          {{#with second}}
          <h2>{{title}}</h2>
          <p>{{text}}</p>
          {{#with button_text}}
          <button class='btn btn-secondary'>{{solid}}</button>
          <br />
          <br />
          <button class='btn btn-outline-secondary'>{{outline}}</button>
          {{/with}}
          {{/with}}
        </div>
      </div>
      <br />
      <div class='card'>
        <div class='card-body'>
          {{#with subblock}}
          <h3>{{title}}</h3>
          <p>{{text}}</p>
          {{#with button_text}}
          <button class='btn btn-dark'>{{solid}}</button>
          <br />
          <br />
          <button class='btn btn-outline-dark'>{{outline}}</button>
          {{/with}}
          {{/with}}
        </div>
      </div>
      <br />
      {{/with}}
    </div>
  </div>
  <br>
  <div class='card'>
    <div class='card-body'>
      {{#with nested_display}}
      <div class="display-1">{{title}}</div>
      <br>
      <div class='card'>
        <div class='card-body'>
          {{#with children_display}}
          <div class="display-4">{{title}}</div>
          <p>{{text}}</p>
          <button class='btn btn-outline-warning'>{{button.text}}</button>
          {{/with}}
        </div>
      </div>
      <br>
      <h6>
        {{header_text}}
      </h6>
      <button class='btn btn-info'>{{button.text}}</button>
      {{/with}}
    </div>
  </div>
  <br>
  <div class='card'>
    <div class='card-body'>
      {{#with paragraph}}
      <p class="text-uppercase">{{title}}</p>
      <p class="text-lowercase">{{text}}</p>
      <a href="{{button.url}}" class='btn btn-link'>{{button.text}}</a>
      <br>
      <br>
      <div class='card'>
        <div class='card-body'>
          <ul>
            {{#each list_name}}
            <li>
              <a href="{{url}}" class='btn btn-link'>{{text}}</a>
            </li>
            {{/each}}
          </ul>
        </div>
      </div>
      {{/with}}
    </div>
  </div>


</main>